{
  "name": "net-keepalive",
  "version": "1.2.1",
  "description": "Provides high-level access to socket options like TCP_KEEPIDLE, TCP_KEEPINTVL, TCP_KEEPCNT",
  "main": "index.js",
  "scripts": {
    "docs": "jsdoc --debug --verbose -r  . -d docs -c .jsdoc.json README.md",
    "test": "mocha",
    "test-coverage": "nyc mocha",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hertzg/node-net-keepalive.git"
  },
  "os": [
    "freebsd",
    "darwin",
    "linux"
  ],
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "ffi",
    "setsockopt",
    "getsockopt",
    "ref",
    "net",
    "keep-alive",
    "keepalive",
    "keep alive",
    "keep",
    "alive",
    "tcp"
  ],
  "author": "George Kotchlamazashvili <georgedot@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hertzg/node-net-keepalive/issues"
  },
  "homepage": "https://github.com/hertzg/node-net-keepalive",
  "dependencies": {
    "ffi-napi": "^2.4.3",
    "ref-napi": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^13.7.0",
    "codecov": "^3.0.2",
    "mocha": "^2.0.0",
    "mockery": "^2.1.0",
    "nyc": "^11.7.1",
    "should": "^7.0.0",
    "sinon": "^5.0.7",
    "jsdoc": "^3.6.3",
    "minami": "^1.2.3",
    "prettier": "^1.19.1"
  }
}
