{
  "name": "net-keepalive",
  "version": "1.3.3",
  "description": "Provides high-level access to socket options like TCP_KEEPIDLE, TCP_KEEPINTVL, TCP_KEEPCNT",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "docs": "jsdoc -c .jsdoc.json",
    "coverage": "npm run coverage:test && npm run coverage:report",
    "coverage:test": "nyc npm run test",
    "coverage:report": "nyc report --reporter=text-lcov > coverage.lcov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hertzg/node-net-keepalive.git"
  },
  "os": [
    "freebsd",
    "darwin",
    "linux"
  ],
  "files": [
    "lib/",
    "*.d.ts"
  ],
  "engines": {
    "node": ">=4"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "ffi",
    "setsockopt",
    "getsockopt",
    "ref",
    "net",
    "keep-alive",
    "keepalive",
    "keep alive",
    "keep",
    "alive",
    "tcp"
  ],
  "author": "George Kotchlamazashvili <georgedot@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hertzg/node-net-keepalive/issues"
  },
  "homepage": "https://github.com/hertzg/node-net-keepalive",
  "dependencies": {
    "ffi-napi": "^3.0.1",
    "ref-napi": "^2.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/node": "^14.0.20",
    "better-docs": "^2.3.0",
    "husky": "^4.2.5",
    "jsdoc": "^3.6.4",
    "mocha": "^8.0.1",
    "mockery": "^2.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "should": "^13.2.3",
    "sinon": "^9.0.2"
  }
}
