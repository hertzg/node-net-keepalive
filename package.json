{
  "name": "net-keepalive",
  "version": "1.1.1",
  "description": "Provides high-level access to socket options like TCP_KEEPIDLE, TCP_KEEPINTVL, TCP_KEEPCNT",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "test-coverage": "nyc mocha",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hertzg/node-net-keepalive.git"
  },
  "os": [
    "freebsd",
    "darwin",
    "linux",
    "win32"
  ],
  "engines": {
    "node": ">=0.10 <=0.12||>=4 <=8"
  },
  "keywords": [
    "ffi",
    "setsockopt",
    "getsockopt",
    "ref",
    "net",
    "keep-alive",
    "keepalive",
    "keep alive",
    "keep",
    "alive",
    "tcp"
  ],
  "author": "George Kotchlamazashvili <georgedot@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hertzg/node-net-keepalive/issues"
  },
  "homepage": "https://github.com/hertzg/node-net-keepalive",
  "dependencies": {
    "ffi": "^2.2.0",
    "ref": "^1.3.5"
  },
  "devDependencies": {
    "@types/node": "^10.0.6",
    "codecov": "^3.0.2",
    "mocha": ">=2.0.0",
    "mockery": "^2.1.0",
    "nyc": "^11.7.1",
    "should": ">=7.0.0",
    "sinon": "^5.0.7"
  }
}
